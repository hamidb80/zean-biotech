{
  "name": "zeanbiotech",
  "version": "0.0.3",
  "type": "module",
  "description": "zean biotech official website",
  "main": "main.js",
  "scripts": {
    "dev": "nodemon --experimental-json-modules src/main.js",
    "style-watch": "less-watch-compiler.cmd  ./styles/  static/dist/ app.less",
    "build": "lessc ./styles/app.less  static/dist/app.css",
    "start": "node --experimental-json-modules src/main.js",
    "update": "fandogh image publish --version 0.6",
    "deploy": "fandogh service apply -f fservice.yml"
  },
  "keywords": [
    "website",
    "zean",
    "biotech",
    "zeanbiotech"
  ],
  "author": "hamidb80",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "less": "^4.1.1",
    "nodemon": "^2.0.12"
  }
}
